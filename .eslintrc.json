{
  "eslintConfig": {
    "extends": [
      "react-app",
      "shared-config"
    ],
    "env": {
      "cypress/globals": true
    },
    "plugins": [
      "cypress"
    ],
    "rules": {
      "additional-rule": "warn"
    },
    "overrides": [
      {
        "files": [
          "**/*.ts?(x)"
        ],
        "rules": {
          "additional-typescript-only-rule": "warn"
        }
      }
    ]
  }
}